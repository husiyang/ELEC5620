<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.elec5619.recipeweb.mapper.UserChatroomMapper">

    <resultMap id = "UserChatroomMap" type="com.elec5619.recipeweb.bean.UserChatroom">
        <id column="id" property="id"></id>
        <result column="memberid" property="memberid"></result>
        <result column="chatroomid" property="chatroomid"></result>
    </resultMap>

    <select id="findByMemberid" resultMap="UserChatroomMap" parameterType="java.lang.Integer">
        select *
        from user_chatroom
        where memberid = #{memberid}
    </select>
</mapper>
